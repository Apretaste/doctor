<div class="row">
    <div class="col s12">
        <h2>No hay respuesta</h2>
        <p>No se encontró respuesta a su búsqueda: <b><%= term %></b></p>

        <% if (Object.keys(similars).length > 0) { %>
            <p>Artículos similares</p>
            <ul>
                <% for(var artid in similars) { %>
                    <li>
                        <a href="#!"
                           onclick="apretaste.send({command: 'DOCTOR SIMILAR', data:{query: '<%= artid %>'}});">
                            <%= similars[artid] %>
                            %>
                        </a>
                    </li>
                <% } %>
            </ul>
        <% } %>
    </div>
</div>

<!-- back floating button -->
<div class="fixed-action-btn">
    <a class="btn-floating btn-large waves-effect waves-light green" href="#!"
       onclick="apretaste.send({'command':'DOCTOR'})">
        <i class="material-icons">arrow_back</i>
    </a>
</div>