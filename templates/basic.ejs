<div class="row">
	<div class="input-field col l12 m12 s12">
        <input id="query" type="text" maxlength="255" class="validate">
        <label for="query">Escriba un sintoma, medicina o malestar</label>
	</div>
</div>

<div class="fixed-action-btn">
	<a class="btn-floating btn-large waves-effect waves-light green" href="#!" onclick="send()"><i class="material-icons">send</i></a>
</div>

<div class="row">
	<div class="col s12">
		<h1><%= term %></h1>
		<%- result %>
		</br>

		<% if (similars.length > 0) { %>
			<h2>Art&iacute;culos similares</h2>
			<ul>
				<% for(var artid in similars) { %>
					<li>
						<a href="#!" onclick="apretaste.send({command: 'DOCTOR ARTICULO', data:{query: '<%= artid %>'}});">
						<%= similars[artid] %> %>
						</a>
					</li>
				<% } %>
			</ul>
		<% } %>
	</div>
</div>
